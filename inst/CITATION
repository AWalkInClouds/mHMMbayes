citHeader("To cite mHMMbayes in publications use:")

citEntry(entry = "PhDThesis",
  title        = "Beyond the average: Choosing and improving statistical methods to optimize inference from complex neuroscience data",
  author       = personList(as.person("Emmeke Aarts?")),
  year         = "2016",
  school       = "Amsterdam: Vrije Universiteit",

  textVersion  =
  paste("Emmeke Aarts (2016).",
        "Beyond the average: Choosing and improving statistical methods to optimize inference from complex neuroscience data.",
        "Amsterdam: Vrije Universiteit")
)
